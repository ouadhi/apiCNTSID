create table deficit
  (
    numero_vi varchar(20) not null ,
    date_accostage datetime year to minute,
    code_bur smallint,
    an_manif datetime year to year,
    num_manif integer,
    nb_tcs smallint
  );


  create table voyage_gros
    (
      numero_vi varchar(20) not null ,
      date_accostage datetime year to minute,
      code_bur smallint,
      an_manif datetime year to year,
      num_manif integer,
      tc_debarque smallint,
      tc_manifeste smallint
    );


