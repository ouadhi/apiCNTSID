              date_arrivee datetime year to minute,
              date_accostage datetime year to minute,
              code_bur smallint,
              an_manif datetime year to year,
              num_manif integer,
              num_ligne smallint,
              num_group smallint,
              etat_lign char(1),
              num_magasin smallint
            );
